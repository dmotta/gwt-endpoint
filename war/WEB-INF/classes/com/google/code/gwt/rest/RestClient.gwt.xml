<?xml version="1.0" encoding="UTF-8"?>
<module>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.http.HTTP' />
	<inherits name='com.google.gwt.jsonp.Jsonp' />

	<source path='client' />

	<generate-with
		class="com.google.code.gwt.rest.rebind.RestRequestFactoryGenerator">
		<when-type-assignable
			class='com.google.code.gwt.rest.client.RestRequestFactory' />
	</generate-with>

	<generate-with class="com.google.code.gwt.rest.rebind.BeanConverterGenerator">
		<when-type-assignable class='com.google.code.gwt.rest.client.BeanConverter' />
	</generate-with>

</module>
